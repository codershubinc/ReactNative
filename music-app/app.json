{
  "expo": {
    "name": "music-app",
    "slug": "music-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/music_app.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/music_app.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000" 
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/music_app.png",
        "backgroundColor": "#000000"
      },
      "package": "com.swapnil.ing.in.musichub"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c0a86afc-9674-41c9-9d40-bc9abd3d1cf2"
      }
    }
  }
}
